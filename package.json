{
  "name": "accuweather-client",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergsavoniuk/accuweather-client.git"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "i18next": "^15.0.4",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.2.0",
    "react-lazyload": "^2.5.0",
    "react-scripts": "2.1.3",
    "react-switch": "^4.0.1",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=src/ react-scripts start",
    "build-sw": "node ./src/sw-build.js",
    "build": "rimraf -rf build && cross-env NODE_PATH=src/ react-scripts build && npm run build-sw",
    "build:run": "yarn build && node ./src/server.js",
    "format": "prettier \"src/**/*.{js,jsx}\" --write",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "express": "^4.17.3",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "workbox-build": "^4.3.1"
  }
}
